{
  "name": "sd-tools",
  "dependencies": {
    "axios": "^1.6.2",
    "configstore": "^6.0.0",
    "image-size": "^1.0.2",
    "jsonschema": "^1.4.1",
    "png-chunk-text": "^1.0.0",
    "png-chunks-extract": "^1.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/configstore": "^6.0.2",
    "@types/node": "^20.9.0",
    "@types/png-chunk-text": "^1.0.3",
    "@types/png-chunks-extract": "^1.0.2",
    "@types/yargs": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^2.4.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.62.0"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node16 --outfile=dist/index.js",
    "start": "node dist/index.js",
    "schema": "yarn schema:config && yarn schema:queue && yarn schema:rename",
    "schema:queue": "typescript-json-schema ./tsconfig.json IPrompts --required --noExtraProps --out src/commons/schema/queue.json",
    "schema:rename": "typescript-json-schema ./tsconfig.json IRenameConfig --required --noExtraProps --out src/commons/schema/rename.json"
  }
}
