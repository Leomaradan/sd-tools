{
  "name": "@leomaradan/sd-tools",
  "version": "1.1.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leomaradan/sd-tools.git"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.4",
    "axios": "^1.6.2",
    "configstore": "^6.0.0",
    "htmlparser2": "^9.1.0",
    "image-size": "^1.0.2",
    "jsonschema": "^1.4.1",
    "png-chunk-text": "^1.0.0",
    "png-chunks-extract": "^1.0.0",
    "table": "^6.8.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/configstore": "^6.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.9.0",
    "@types/png-chunk-text": "^1.0.3",
    "@types/png-chunks-extract": "^1.0.2",
    "@types/yargs": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^2.4.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.62.0"
  },
  "bin": {
    "sd-tools": "bin.cjs"
  },
  "main": "./dist/index.cjs",
  "exports": {
    "./package.json": "./package.json",
    "./schema/queue.json": "./schema/queue.json",
    "./schema/rename.json": "./schema/rename.json",
    ".": {
      "require": {
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/index.cjs",
    "bin.cjs"
  ],
  "type": "module",
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "build": "yarn schema && esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=build/index.cjs",
    "build:prod": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" src/index.ts --bundle --platform=node --target=node20  --minify --outfile=dist/index.cjs",
    "watch": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=build/index.cjs --watch",
    "start": "node build/index.cjs",
    "test": "jest --coverage",
    "schema": "yarn schema:queue && yarn schema:rename",
    "schema:queue": "typescript-json-schema ./tsconfig.json IPrompts --required --noExtraProps --out schema/queue.json --titles --useTypeOfKeyword",
    "schema:rename": "typescript-json-schema ./tsconfig.json IRenameConfig --required --noExtraProps --out schema/rename.json --titles --useTypeOfKeyword"
  }
}
